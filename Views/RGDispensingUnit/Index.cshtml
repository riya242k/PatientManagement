@model IEnumerable<RGPatients.Models.DispensingUnit>

@{
    ViewData["Title"] = "List Of Dispensing Units";
}

<h1>List Of Dispensing Units</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DispensingCode)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(model => item.DispensingCode)
                </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DispensingCode">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DispensingCode">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DispensingCode">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
